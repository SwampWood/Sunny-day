CREATE TABLE MeteorologicalData (
    StationIndex VARCHAR(5),
    YearGMT INT,
    MonthGMT TINYINT,
    DayGMT TINYINT,
    TimeGMT TINYINT,
    YearLocal INT,
    MonthLocal TINYINT,
    DayLocal TINYINT,
    TimeLocal TINYINT,
    PDZVTimeNumber TINYINT,
    LocalTime TINYINT,
    TimezoneNumber TINYINT,
    MeteorologicalDayStart TINYINT,
    HorizontalVisibility DECIMAL(4,1),
    QualityIndicator_HorizontalVisibility TINYINT,
    SignGreaterThanIndicator_HorizontalVisibility TINYINT,
    TotalCloudCover TINYINT,
    QualityIndicator_TotalCloudCover TINYINT,
    LowerCloudAmount TINYINT,
    QualityIndicator_LowerCloudAmount TINYINT,
    UpperCloudForm VARCHAR(2),
    QualityIndicator_UpperCloudForm TINYINT,
    MiddleCloudForm VARCHAR(2),
    QualityIndicator_MiddleCloudForm TINYINT,
    VerticalCloudForm VARCHAR(2),
    QualityIndicator_VerticalCloudForm TINYINT,
    StratusClouds VARCHAR(2),
    QualityIndicator_StratusClouds TINYINT,
    RainClouds VARCHAR(2),
    QualityIndicator_RainClouds TINYINT,
    CloudBaseHeight INT,
    QualityIndicator_CloudBaseHeight TINYINT,
    CloudBaseHeightIndicator TINYINT,
    CloudsBelowStation TINYINT,
    QualityIndicator_CloudsBelowStation TINYINT,
    WeatherBetweenTimes VARCHAR(2),
    QualityIndicator_WeatherBetweenTimes TINYINT,
    WeatherObservationTime VARCHAR(2),
    QualityIndicator_WeatherObservationTime TINYINT,
    WindDirection INT,
    AverageWindSpeed DECIMAL(4,1),
    QualityIndicator_AverageWindSpeed TINYINT,
    SignGreaterThanIndicator_AverageWindSpeed TINYINT,
    MaxWindSpeed DECIMAL(4,1),
    QualityIndicator_MaxWindSpeed TINYINT,
    SignGreaterThanIndicator_MaxWindSpeed TINYINT,
    PrecipitationAmount DECIMAL(4,2),
    QualityIndicator_PrecipitationAmount TINYINT,
    GroundSurfaceTemperature DECIMAL(5,1),
    QualityIndicator_GroundSurfaceTemperature TINYINT,
    MinGroundSurfaceTemperature DECIMAL(5,1),
    QualityIndicator_MinGroundSurfaceTemperature TINYINT,
    MinGroundSurfaceTemperatureBetweenTimes DECIMAL(5,1),
    QualityIndicator_MinGroundSurfaceTemperatureBetweenTimes TINYINT,
    MaxGroundSurfaceTemperatureBetweenTimes DECIMAL(5,1),
    QualityIndicator_MaxGroundSurfaceTemperatureBetweenTimes TINYINT,
    ShakenThermometerGroundTemperature DECIMAL(5,1),
    QualityIndicator_ShakenThermometerGroundTemperature TINYINT,
    DryBulbTemperature DECIMAL(5,1),
    QualityIndicator_DryBulbTemperature TINYINT,
    WetBulbTemperature DECIMAL(5,1),
    QualityIndicator_WetBulbTemperature TINYINT,
    IcePresenceIndicator TINYINT,
    MinAirTemperature DECIMAL(5,1),
    QualityIndicator_MinAirTemperature TINYINT,
    MaxAirTemperature DECIMAL(5,1),
    QualityIndicator_MaxAirTemperature TINYINT,
    ShakenThermometerAirTemperature DECIMAL(5,1),
    QualityIndicator_ShakenThermometerAirTemperature TINYINT,
    WaterVapourPartialPressure DECIMAL(5,2),
    MeasurementAccuracyIndicator TINYINT,
    RelativeHumidity TINYINT,
    SaturationDeficit DECIMAL(5,2),
    QualityIndicator_SaturationDeficit TINYINT,
    DewPointTemperature DECIMAL(5,1),
    QualityIndicator_DewPointTemperature TINYINT,
    StationLevelPressure DECIMAL(6,1),
    QualityIndicator_StationLevelPressure TINYINT,
    SeaLevelPressure DECIMAL(6,1),
    QualityIndicator_SeaLevelPressure TINYINT,
    BarometricTendencyCharacteristics VARCHAR(2),
    QualityIndicator_BarometricTendencyCharacteristics TINYINT,
    BarometricTendencyMagnitude DECIMAL(4,1),
    QualityIndicator_BarometricTendencyMagnitude TINYINT
    SeaLevelPressure DECIMAL(6,1),
    QualityIndicator_SeaLevelPressure TINYINT,
    BarometricTendencyCharacteristics VARCHAR(2),
    QualityIndicator_BarometricTendencyCharacteristics TINYINT,
    BarometricTendencyMagnitude DECIMAL(4,1),
    QualityIndicator_BarometricTendencyMagnitude TINYINT);
